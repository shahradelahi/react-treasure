{
  "name": "react-treasure",
  "description": "Share and manage your data across children components",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup"
  },
  "packageManager": "pnpm@8.11.0",
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.2.44",
    "@types/react-dom": "^18.2.17",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shahradelahi/react-treasure.git"
  },
  "keywords": [
    "react",
    "treasure",
    "context",
    "provider",
    "consumer"
  ],
  "author": "Shahrad Elahi <shahrad@litehex.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shahradelahi/react-treasure/issues"
  },
  "homepage": "https://github.com/litehex/mongodb#readme"
}
